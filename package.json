{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "typings-install": "node node_modules/typings/dist/bin.js install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server"
  },
  "contributors": [
    "Suguru Inatomi <laco0416@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "~0.16.2",
    "codelyzer": "0.0.18",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.2",
    "source-map-loader": "^0.1.5",
    "tslint": "^3.8.1",
    "tslint-loader": "^2.1.4",
    "typescript": "~1.8.9",
    "typings": "^0.7.9",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "angular2": "github:angular/angular#2.0.0-build.f5f0038.js",
    "core-js": "^2.2.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  },
  "keywords": [
    "Angular2",
    "angular2-seed",
    "official angular 2 seed",
    "official angular2 seed"
  ]
}
